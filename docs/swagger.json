            }
        },
        "dto.UpdateUserProfileDTO": {
            "type": "object",
            "required": [
                "birth_date",
                "birth_place",
                "gender",
                "institusi",
                "name",
                "nim"
            ],
            "properties": {
                "birth_date": {
                    "type": "string"
                },
                "birth_place": {
                    "type": "string"
                },
                "gender": {
                    "type": "string"
                },
                "institusi": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "nim": {
                    "type": "string"
                }
            }
