                "birth_date": {
                    "type": "string"
                },
                "birth_place": {
                    "type": "string"
                },
                "data_has_verified": {
                    "type": "boolean"
                },
                "email": {
                    "type": "string"
                },
                "gender": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "institusi": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "nim": {
                    "type": "string"
                },
                "profile_has_updated": {
                    "type": "boolean"
                },
                "role": {
                    "type": "string"
                }
            }
        },
        "dto.ValidateGoogleIdTokenDTO": {
            "type": "object",
            "required": [
                "google_id_token"
            ],
            "properties": {
                "google_id_token": {
                    "type": "string"
                }
            }
        },
        "helper.Response": {
            "type": "object",
            "properties": {
                "data": {},
                "errors": {},
                "message": {
                    "type": "string"
                },
                "success": {
                    "type": "boolean"
                }
            }
        }
    }
}